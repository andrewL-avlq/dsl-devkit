formatter for org.example.A

RuleA {
  ";" : linewrap after;
  =a : linewrap before;
  rule : linewrap before;
  "-"(1,4) : space "" after;
  group 1 : linewrap before;
  group/*@G1*/ 1 => group/*@G12*/ 2 => group/*@G121*/ 1 => group/*@G1212*/ 2 {
    "-" : column 1 after;
  }
  group 1 => group 2 => group 1 {
    "-"(1,2) : linewrap before;
  }
}

RuleAA {
  @RuleA : linewrap before;
}